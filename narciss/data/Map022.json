eters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「还有人能看见，等再晚一点切吧。」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["…并不是放弃了啊…"]},{"code":117,"indent":0,"parameters":[1]},{"code":231,"indent":0,"parameters":[1,"sora02",0,0,0,0,100,100,255,0]},{"code":222,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"umitori","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["灼热的阳光，潮湿的海风。以及海浪的气味。"]},{"code":401,"indent":0,"parameters":["有时候，也会靠来几只海鸥。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["在这样的海滩，我们稍晚地开始了午餐的食用。"]},{"code":117,"indent":0,"parameters":[1]},{"code":231,"indent":0,"parameters":[1,"umibe",0,0,0,0,100,100,255,0]},{"code":222,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0270","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「啊呜啊呜，你妈妈啊，厨艺真好呢。」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0223","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「…今天特别用心做的。」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0271","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「这样啊，是位好妈妈呢。」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0124","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「…嗯。」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["一边闲聊着一边吃着便当。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"inden