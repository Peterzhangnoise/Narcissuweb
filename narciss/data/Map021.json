ode":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0203","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「哈哈，怎么可能这么快啊。」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["然后，毫无顾及地对我笑着。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["…虽然说她是虔诚的天主教徒…"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["不过像是在教堂祈祷的千寻那样的感觉，"]},{"code":401,"indent":0,"parameters":["至少我没办法想象。"]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0204","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「这里是荒野…所以花才会开放喔。」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0177","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「嗯？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0205","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「…必开花繁盛，乐上加乐而且欢呼。」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0206","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["「知道吗？这是以赛亚书的句段喔。」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["我只能摇头。"]},{"code":401,"indent":0,"parameters":["虽然不是很了解，不过似乎是类似经文的东西吧。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["因为刚刚，思考着那样的事，"]},{"code":401,"indent":0,"parameters":["突然被姬子所说的话吓到了。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["然后，考虑着姬子所说的话的时候…"]},{"code":250,"indent":0,"parameters":[{"name":"asi02","volume":90,"pitch":100,"pan":0}]},{"code":117,"indent":0,"parameters":[3]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「呼，今天真热呢。」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["从某处跑来的护士。"]},{"code":401,"indent":0,"parameters":["似乎我从没见过，也不知是哪个楼层的人。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「谢谢你喔，姬子。」"]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0207","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「啊，只是顺便也帮其他的花浇下水罢了。」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「嗯，选哪一个好呢？"]},{"code":401,"indent":0,"parameters":["　想稍微带一点回去…」"]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0208","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「…几楼用？」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「３楼的Ｘ光等待室。」"]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0209","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「那么，盆栽的也可以呢。」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["说着，把黄色和白色的三色菫，"]},{"code":401,"indent":0,"parameters":["用铲子刨了几朵放进了塑胶袋里。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「谢谢，那么失陪了。」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["把塑胶袋拿到手里，"]},{"code":401,"indent":0,"parameters":["再次往着建筑物方向渐渐离去的护士。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["告别的时候用手划了个十字。"]},{"code":401,"indent":0,"parameters":["那个护士大概也是天主教的人吧？"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["然后，有点不太理解…"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["看来这个花坛，是由姬子负责，"]},{"code":401,"indent":0,"parameters":["择选植物种植类型的也说不一定。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["这也不是因为她是７楼的病人，"]},{"code":401,"indent":0,"parameters":["而是对于医院来说，已经公认由她来做的感觉。"]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0178","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「…刚刚的护士也认识？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0210","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「嗯，还认识其他很多的人喔。」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["虽然听说她是原陪护，"]},{"code":401,"indent":0,"parameters":["只是那样就有办法达到这样的地步？"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["只要想着这里是天主教会的医院…"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["看到刚刚虔诚的天主教徒，"]},{"code":401,"indent":0,"parameters":["大概，还是有所关系的吧。"]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0211","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「呵呵，怎么了？做出这么不可思议的表情？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0179","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「唉？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0212","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「也不是，得到了什么特别的权限喔。」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0213","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「我呢，是原陪护。现在是７楼的病人…」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0214","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「只是，因为履历比较长，"]},{"code":401,"indent":0,"parameters":["　得到一点特别的待遇的程度啦。」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["然后，说着在我这年纪的时候，"]},{"code":401,"indent":0,"parameters":["常常进出这里参加公益活动等等。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["然后，说着在我这年纪的时候，"]},{"code":401,"indent":0,"parameters":["常常进出这里参加公益活动等等。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["在这句话中的”履历比较长”，"]},{"code":401,"indent":0,"parameters":["应该不光光是指陪护吧。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["说到这里以前也有提过，"]},{"code":401,"indent":0,"parameters":["天主教徒经常积极的参加各种福利活动。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["这样想的话，恐怕就和千寻说的一样，"]},{"code":401,"indent":0,"parameters":["姬子是虔诚的信徒也说不一定吧。"]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0215","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「怎么了？还有什么想问的吗？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0180","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「为什么姬子，从不祈祷呢？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0216","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「祈祷？这样的我吗？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0181","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「因为，你不是虔诚的天主教徒吗？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0217","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「………………」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0218","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「是呢…如果不喜欢伪天主教徒的话…」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0219","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「…”原”天主教徒也可以喔。」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["这意味着肯定过去，否定现在的意思。"]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0182","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「那么，已经没有再祈祷了吗？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0220","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「没有。」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["干脆地回答道。"]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0221","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「顺便说起来…」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0222","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「有祈祷的话，不也就代表说有诅咒吗？」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["有点不太能够理解…"]},{"code":401,"indent":0,"parameters":["突然就这样问，我也没办法把它们连贯起来。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["况且在这问题之前，"]},{"code":401,"indent":0,"parameters":["我连祈祷和诅咒有什么不同都不知道。"]},{"code":117,"indent":0,"parameters":[3]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0223","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「对了，比起这个，决定了明天就去。」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0172","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「嗯？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0224","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「你看，之前不是失败了吗。」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["…大概，是指凤梨树的事吧？"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["结果那天，虽然到了车站，不过却只有放弃回到医院。"]},{"code":401,"indent":0,"parameters":["然后决定改天再出发…"]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0184","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「不过，要怎么去？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0225","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「没问题啦，放心交给姐姐吧。」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"hime1_0226","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「总之，因为很难得，便当，可不要忘记了喔。」"]},{"code":251,"indent":0,"parameters":[]},{"code":117,"indent":0,"parameters":[3]},{"code":242,"indent":0,"parameters":[2]},{"code":117,"indent":0,"parameters":[1]},{"code":231,"indent":0,"parameters":[1,"sora06",0,0,0,0,100,100,255,0]},{"code":222,"indent":0,"parameters":[]},{"code":241,"indent":0,"parameters":[{"name":"2mi02","volume":70,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["太阳西下的时候。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["今天也同往常一样，两个人围在餐桌前。"]},{"code":117,"indent":0,"parameters":[1]},{"code":231,"indent":0,"parameters":[1,"heya1",0,0,0,0,100,100,255,0]},{"code":222,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"mama_033","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「嗯？明天的便当，两人份？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0186","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「嗯，可以吗…」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["明天，因为要和姬子出游，"]},{"code":401,"indent":0,"parameters":["所以拜托准备两人份的便当。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["不过，虽说我最近身体状况都还不错，"]},{"code":401,"indent":0,"parameters":["不过要是妈妈说不行的话我也打算放弃。"]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0187","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「…果然不行？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"mama_034","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「真是的，怎么会不行呢。」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"mama_035","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「妈妈，会鼓足干劲帮你们准备的。」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["把我的担心抛在一边，简单地说着ＯＫ的妈妈。"]},{"code":401,"indent":0,"parameters":["倒不如说比平常还更高兴的样子。"]},{"code":250,"indent":0,"parameters":[{"name":"mama_036","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「那个，你朋友姬子，"]},{"code":401,"indent":0,"parameters":["　有什么不喜欢的食物吗？」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0188","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「…花椰菜。」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"mama_037","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「我知道了，那么会记得不放那个的…」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"mama_038","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「对了对了，下次方便的话，"]},{"code":401,"indent":0,"parameters":["　也叫她来我们家玩玩吧。」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0189","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「…嗯…」"]},{"code":251,"indent":0,"parameters":[]},{"code":117,"indent":0,"parameters":[1]},{"code":231,"indent":0,"parameters":[1,"heya2y2",0,0,0,0,100,100,255,0]},{"code":222,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0168","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「哈…」"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["我仰倒在床上，大大的出了一口气。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["…妈妈一定，认为姬子是，"]},{"code":401,"indent":0,"parameters":["普通的入院患者，或是护士那样想的吧。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["至少，不会想到是７楼的病人。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["…土豆也一样，姬子的事情也一样。"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["妈妈越是高兴，不过…"]},{"code":117,"indent":0,"parameters":[3]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0191","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「…我的心越是痛苦。」"]},{"code":251,"indent":0,"parameters":[]},{"code":250,"indent":0,"parameters":[{"name":"setsumi_0192b","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["「这就是，称作”罪恶感”的东西吧…」"]},{"code":251,"indent":0,"parameters":[]},{"code":117,"indent":0,"parameters":[3]},{"code":242,"indent":0,"parameters":[2]},{"code":201,"indent":0,"parameters":[0,12,1,0,0,0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":4,"walkAnime":true}],"x":0,"y":0}
